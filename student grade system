#include <iostream>
using namespace std;

struct Student {
    string name;
    int marks;
};

int main() {
    Student s[3];
    for(int i = 0; i < 3; ++i) {
        cout << "Enter name and marks for student " << i+1 << ": ";
        cin >> s[i].name >> s[i].marks;
    }

    cout << "\nStudent Grades:\n";
    for(int i = 0; i < 3; ++i) {
        cout << s[i].name << ": " << (s[i].marks >= 50 ? "Pass" : "Fail") << endl;
    }
    return 0;
}
